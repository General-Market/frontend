{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/docs",
      "destination": "https://generalmarket.mintlify.dev/docs"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://generalmarket.mintlify.dev/docs/:match*"
    }
  ]
}
